{
  "name": "@netlify/sdk--extension-api-client",
  "version": "2.12.6",
  "author": "Netlify, Inc.",
  "description": "A TypeScript client used to interact with the extension API.",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "dependencies": {
    "@netlify/blobs": "^10.0.4",
    "camelize-ts": "^3.0.0",
    "openapi-fetch": "^0.13.4",
    "snakify-ts": "^2.3.0",
    "type-fest": "^4.35.0",
    "wretch": "^2.7.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/recommended": "^1.0.5",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^18.18.12",
    "msw": "^2.4.1",
    "openapi-typescript": "^7.6.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.0"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "clean": "rm -rf dist/ && rm -f *.tsbuildinfo",
    "generate:netlify_api_types": "openapi-typescript vendor/bitballoon-openapi/openapi-external.json -o ./src/generated/bitballoon.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --incremental",
    "typecheck:watch": "tsc --noEmit --incremental --watch",
    "watch": "pnpm run build:watch"
  }
}