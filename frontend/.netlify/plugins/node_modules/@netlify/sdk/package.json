{
  "name": "@netlify/sdk",
  "version": "3.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">= 18.19.0"
  },
  "bin": {
    "ntli": "./bin.js",
    "netlify-extension": "./bin.js",
    "netlify-integration": "./bin-deprecated.js"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/api/netlify.d.ts",
      "default": "./dist/api/netlify.js"
    },
    "./bin": {
      "default": "./dist/bin/index.js"
    },
    "./commands": {
      "default": "./dist/cli/commands/index.js"
    },
    "./cli-utils": {
      "default": "./dist/cli/util.js"
    },
    "./model": {
      "types": "./dist/extension/connect/model/index.d.ts",
      "default": "./dist/extension/connect/model/index.js"
    },
    "./connector": {
      "types": "./dist/extension/connect/types/index.d.ts",
      "default": "./dist/extension/connect/types/index.js"
    },
    "./ui": {
      "types": "./dist/ui/main.d.ts",
      "default": "./dist/ui/main.js"
    },
    "./ui/functions": {
      "types": "./dist/ui/functions.d.ts",
      "default": "./dist/ui/functions.js"
    },
    "./ui/functions/trpc": {
      "types": "./dist/ui/functions-trpc.d.ts",
      "default": "./dist/ui/functions-trpc.js"
    },
    "./ui/functions/trpc/unstable": {
      "types": "./dist/ui/functions-trpc-unstable.d.ts",
      "default": "./dist/ui/functions-trpc-unstable.js"
    },
    "./ui/react": {
      "types": "./dist/ui/react.d.ts",
      "default": "./dist/ui/react.js"
    },
    "./ui/react/unstable": {
      "types": "./dist/ui/react-unstable.d.ts",
      "default": "./dist/ui/react-unstable.js"
    },
    "./ui/react/components": {
      "types": "./dist/ui/react-components.d.ts",
      "default": "./dist/ui/react-components.js"
    },
    "./ui/react/tailwind-config": {
      "types": "./dist/ui/tailwind-config.d.ts",
      "default": "./dist/ui/tailwind-config.js"
    },
    "./ui/react/core.css": {
      "import": "./dist/ui/core.css"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@graphql-tools/stitch": "^9.0.3",
    "@graphql-tools/utils": "^10.1.2",
    "@graphql-tools/wrap": "^10.0.1",
    "@netlify/functions": "^4.1.10",
    "@netlify/integrations": "^0.6.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/configstore": "^6.0.2",
    "camelcase": "^8.0.0",
    "chalk": "^5.2.0",
    "commander": "^12.1.0",
    "configstore": "6.0.0",
    "dataloader": "^2.2.3",
    "env-paths": "^3.0.0",
    "esbuild": "^0.25.0",
    "esbuild-plugin-copy": "^2.1.1",
    "escalade": "^3.2.0",
    "execa": "^9.3.0",
    "fs-extra": "^11.1.0",
    "graphql-scalars": "^1.22.5",
    "js-xxhash": "^3.0.1",
    "js-yaml": "^4.1.0",
    "lodash.camelcase": "^4.3.0",
    "moize": "^6.1.6",
    "normalize-package-data": "^7.0.0",
    "open": "^9.1.0",
    "outdent": "^0.8.0",
    "safe-stable-stringify": "^2.4.3",
    "semver": "^7.7.2",
    "string-argv": "^0.3.2",
    "ts-morph": "^25.0.1",
    "typescript": "^5.4.3",
    "wretch": "^2.7.0",
    "zod": "^3.22.4",
    "zod-metadata": "^1.1.0",
    "zod-to-json-schema": "^3.24.5",
    "zod-validation-error": "^2.0.0",
    "@netlify/content-engine": "1.9.1",
    "@netlify/sdk--extension-api-client": "2.12.6",
    "@netlify/sdk--ui-functions": "1.10.0",
    "@netlify/sdk--ui-core": "1.14.0",
    "@netlify/sdk--ui-react": "1.23.0"
  },
  "devDependencies": {
    "@netlify/build": "^33.5.1",
    "@pnpm/exec": "^2.0.0",
    "@stackbit/types": "^0.11.12",
    "@trpc/server": "^11.4.3",
    "@types/chance": "^1.1.5",
    "@types/fs-extra": "^11.0.3",
    "@types/js-yaml": "^4.0.8",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/node": "~20.9.5",
    "@types/normalize-package-data": "^2.4.4",
    "@types/semver": "^7.7.0",
    "@types/uuid": "^9.0.0",
    "chance": "^1.1.11",
    "get-bin-path": "^11.0.0",
    "graphql": "^16.8.1",
    "lodash.kebabcase": "^4.1.1",
    "msw": "^2.3.0",
    "netlify-cli": "^21.6.0",
    "publint": "^0.2.5",
    "utility-types": "^3.11.0",
    "uuid": "^9.0.0",
    "vitest": "^3.2.4",
    "yaml": "^2.5.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "watch": "pnpm run build:watch",
    "clean": "rm -rf dist/ && rm -f *.tsbuildinfo",
    "lint": "publint",
    "postbuild": "cp ./src/ui/core.css ./dist/ui/core.css",
    "test": "vitest",
    "test:build": "vitest test/build/ test/build-events",
    "test:build_event_handlers": "vitest test/build-event-handlers",
    "test:client": "vitest test/netlify-client",
    "test:connect:all": "vitest test/connect*",
    "test:connect": "vitest test/connect/",
    "test:connect-group-2": "vitest test/connect-group-2/",
    "test:connect-unit": "vitest test/connect-unit/",
    "test:dev": "vitest test/dev/",
    "test:installation": "vitest test/installation/",
    "test:unit": "vitest test/connect-unit src/**/*test*",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  }
}